<!DOCTYPE html>
<html lang="en" style="background-color: #f3f3f3">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>{% block title %}Cotupap - Admin Dashboard{% endblock %}</title>
    {% block stylesheets %}
        <!-- plugins:css -->
        <link rel="stylesheet" href="{{ asset('assets/DashBundle/vendors/iconfonts/simple-line-icon/css/simple-line-icons.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/DashBundle/vendors/iconfonts/flag-icon-css/css/flag-icon.min.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/DashBundle/vendors/css/vendor.bundle.base.css') }}">
        <link rel="stylesheet" href="{{ asset('assets/DashBundle/vendors/css/vendor.bundle.addons.css') }}">
        <!-- endinject -->
        <!-- plugin css for this page -->
        <!-- End plugin css for this page -->
        <!-- inject:css -->
        <link rel="stylesheet" href="{{ asset('assets/DashBundle/css/style.css') }}">
        <!-- endinject -->
        <link rel="shortcut icon" href="{{ asset('assets/DashBundle/images/favicon.ico') }}" />
    {% endblock %}
</head>
<body style="max-width: 1774px;margin-left: auto; margin-right: auto;">
<div class="container-scroller">
    {% include '@Admin/partials/horizontal-navbar.html.twig' %}
    <div class="container-fluid page-body-wrapper">
        <div class="main-panel">
            <div class="content-wrapper">
                {% block body %}{% endblock %}
            </div>
            {% include '@Admin/partials/footer.html.twig' %}
        </div>
    </div>
    {% block javascripts %}
        <!-- plugins:js -->
        <script src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
        <script src="{{ asset('assets/DashBundle/vendors/js/vendor.bundle.base.js') }}"></script>
        <script src="{{ asset('assets/DashBundle/vendors/js/vendor.bundle.addons.js') }}"></script>
        <!-- endinject -->
        <!-- Plugin js for this page-->
        <!-- End plugin js for this page-->
        <!-- inject:js -->
        <script src="{{ asset('assets/DashBundle/js/template.js') }}"></script>
        <!-- endinject -->
        <!-- Custom js for this page-->
        <script src="{{ asset('assets/DashBundle/js/dashboard.js') }}"></script>
        <script src="{{ asset('assets/DashBundle/js/todolist.js') }}"></script>
        <!-- End custom js for this page-->
    {% endblock %}
</div>
</body>
</html>
